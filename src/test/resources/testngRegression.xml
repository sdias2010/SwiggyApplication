<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Swiggy Regression Suite">
    <listeners>
        <listener class-name="com.simplilearn.listeners.TestngListener"></listener>
    </listeners>
    <test name="Regression Test on Chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.simplilearn.TestRunnerRegression"></class>
        </classes>
    </test>
    <test name="Regression Test on FireFox">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="com.simplilearn.TestRunnerRegression"></class>
        </classes>
    </test>
</suite>